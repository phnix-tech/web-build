{
  "name": "@web-io/build",
  "version": "0.5.0",
  "description": "web builds scripts",
  "author": "leejefy <050934@gmail.com>",
  "homepage": "https://gitee.com/phoenix-tech/web-build",
  "scripts": {
    "clean": "rimraf ./lib",
    "build": "run-s clean pre-build build-task",
    "test": "npx web-io-jest",
    "check-versions": "node ./build/check-versions.js",
    "pre-build": "run-s check-versions lint test",
    "build-task": "run-p build-ts",
    "build-ts": "tsc --project ./tsconfig.json",
    "lint": "run-p eslint",
    "lint:fix": "run-p eslint:fix",
    "eslint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .js,.ts ./",
    "eslint:fix": "eslint --fix --cache --cache-location ./node_modules/.cache/.eslintcache --ext .js,.ts ./"
  },
  "dependencies": {
    "mkdirp": "0.5.1",
    "tar": "4.4.8",
    "chalk": "2.4.2",
    "semver": "5.6.0",
    "shelljs": "0.8.3",
    "portfinder": "1.0.20",
    "webpack-dev-mock": "1.0.1",
    "@web-io/common": "file:deps/web-io-common-1.2.0-dev1.tgz"
  },
  "devDependencies": {
    "typescript": "4.3.5",
    "@types/node": "12.20.15",
    "eslint": "7.29.0",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "@web-io/lint": "1.0.0",
    "@web-io/jest": "file:deps/web-io-jest-1.0.0-dev3.tgz"
  },
  "files": [
    "lib",
    "be",
    "build",
    "scripts",
    "functions.js"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@gitee.com:phoenix-tech/web-build.git"
  },
  "keywords": [
    "web builds",
    "vue cli",
    "react scripts"
  ],
  "engines": {
    "node": ">= 12.16.1",
    "npm": ">= 6.13.4"
  }
}
